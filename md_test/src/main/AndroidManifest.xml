<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="wenld.github.md_test">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data
                    android:host="wenld.github.md_test"
                    android:path="/activity_main"
                    android:scheme="wenld"/>
            </intent-filter>
        </activity>
        <activity android:name=".TextInputLayoutActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".SnackbarActivity" />
        <activity android:name=".ToolBarActivity" />
        <activity android:name=".CoordinatorLayoutActivity" />
        <activity android:name=".BehaviorActivity_1" />
        <activity android:name=".BottomSheetDialogActivity" />
        <activity android:name=".AnimatorActivity" />
    </application>
</manifest>
